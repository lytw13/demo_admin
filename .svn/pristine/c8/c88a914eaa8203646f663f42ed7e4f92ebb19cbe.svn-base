/* 自定义样式 */



/*导航栏用户头像 */
.image > a > #icon {
	width: 100%;
    max-width: 45px;
    height: auto;
}


/* 用户搜索框以及布局 */
.search_form {
    	width:auto;
    	height:100px;
    	border: 1px solid #ccc;
    	border-radius:20px;
    	text-align: center;/*(让div中的内容居中)*/
    	}
	    .search {
	    margin:10px;
	    }
	    .leftContainer {
	    width:30%;
	    border: 1px solid #ccc;
	    float: left;
	    }
	   .rightContainer {
	    width:70%;
	    float: left;
	    }
	    
	    
.errorMessage {
    	color: red;
    	font-weight: 700;
    	}
.error{
    	color: red;
    	}
    	
