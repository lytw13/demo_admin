    <!-- iCheck -->
  	<link rel="stylesheet" th:href="@{/static/css/iCheck/blue.css}">
  </head>
  
<body class="hold-transition login-page">
<div class="login-box">
  <div class="login-logo">
    <b>lytw</b>13
  </div>
  <!-- /.login-logo -->
  <div class="login-box-body">
    <h2 class="headline text-yellow">注册成功</h2>
      <h3><i class="fa fa-warning text-yellow"></i>请前往指定邮箱进行激活.</h3>

      <p>
        在您注册的邮箱进行激活完成后，点击<a th:href="@{/loginPage}">这里</a>进行登录。
      </p>
  </div>
  <!-- /.login-box-body -->
</div>
<!-- /.login-box -->

 <!-- js基础文件-->
    <script th:src="@{/static/js/jquery-3.4.1.js}"></script>
